{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@core/*": [
        "src/app/@core/*"
      ],
      "@shared/*": [
        "src/app/@shared/*"
      ],
      "@shell/*": [
        "src/app/@shell/*"
      ],
      "@env/*": [
        "src/environments/*"
      ],
      "ngd-icons": [
        "dist/ngd-icons"
      ],
      "ngd-icons/*": [
        "dist/ngd-icons/*"
      ],
      "ngd-circular-progress": [
        "dist/ngd-circular-progress"
      ],
      "ngd-circular-progress/*": [
        "dist/ngd-circular-progress/*"
      ]
    }
  }
}
